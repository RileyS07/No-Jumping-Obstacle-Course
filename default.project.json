{
  "name": "NJOC: Rework",
  "tree": {
    "$className": "DataModel",
    "$ignoreUnknownInstances": true,
    
    "ServerStorage": {
      "$className": "ServerStorage",
      "$ignoreUnknownInstances": true,

      "Server": {
        "$className": "Folder",
        "$path": "src/Server",
        "$ignoreUnknownInstances": true
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$ignoreUnknownInstances": true,

      "Client": {
        "$className": "Folder",
        "$path": "src/Client",
        "$ignoreUnknownInstances": true
      },

      "Shared": {
        "$className": "Folder",
        "$path": "src/Shared",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",

      "Bootstrap": {
        "$className": "Folder",
        "$path": "src/ServerBootstrap"
      }
    },

    "ReplicatedFirst": {
      "$className": "ReplicatedFirst",

      "Bootstrap": {
        "$className": "Folder",
        "$path": "src/ClientBootstrap"
      }
    }
  }
}